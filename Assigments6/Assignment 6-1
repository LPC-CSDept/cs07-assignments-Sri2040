num1 = []
for i in range(5):
    num1.append(int(input("Enter Value for first list: ")))

num2 = []
for i in range(5):
     num2.append(int(input("Enter Value for second list: ")))

def sumProduct(a, b):
    ans= []
    for i in range(len(a)):
        ans.append(a[i] * b[i])
    print (ans)
    return sum(ans)

print (sumProduct(num1, num2))
    